{
  "name": "universal-redux-excel",
  "version": "1.0.0",
  "description": "A project to showcase Excel-like spreadsheet features in react and redux universally.",
  "main": "server.js",
  "scripts": {
    "postbuild": "./node_modules/.bin/babel-node server.js",
    "build-dev": "npm run clean && webpack && npm run postbuild",
    "clean": "rm -rf dist && mkdir dist",
    "prod": "SET NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js --progress --colors",
    "build-prod": "npm run clean && npm run prod && npm run postbuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mangla2/universal-redux-excel.git"
  },
  "keywords": [
    "react",
    "redux",
    "node",
    "express",
    "excel-components"
  ],
  "author": "Shubham Mangla",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mangla2/universal-redux-excel/issues"
  },
  "homepage": "https://github.com/mangla2/universal-redux-excel#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "ejs": "^2.5.5",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "jsdom": "^9.10.0",
    "node-localstorage": "^1.3.0",
    "nodemon": "^1.11.0",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-connect": "^5.0.0",
    "redux-logger": "^2.8.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
